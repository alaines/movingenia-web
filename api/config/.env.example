# MOVINGENIA API - Variables de Entorno
# Copia este archivo a .env y configura tus credenciales

# Base de datos
DB_DRIVER=pgsql           # pgsql o mysql
DB_HOST=localhost
DB_PORT=5432              # 5432 para PostgreSQL, 3306 para MySQL
DB_NAME=movingenia_db
DB_USER=your_user
DB_PASS=your_password

# Configuración general
APP_ENV=development       # development o production
APP_DEBUG=true            # true o false
API_VERSION=v1

# CORS (si se necesita)
CORS_ALLOWED_ORIGINS=*    # * o dominio específico
